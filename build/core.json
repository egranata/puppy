[
    {"name" : "ACPICA",
     "src" : "third_party/acpica/src",
     "cflagsAgument" : ["-Wno-error=unused-parameter"]},
    {"name" : "FatFS",
     "src" : "third_party/fatfs"},
    {"name" : "Muzzle",
     "src" : "third_party/muzzle/src",
     "asmflags" : ["-nostartfiles", "-nodefaultlibs", "-Wall", "-Wextra", "-fdiagnostics-color=always", "-nostdlib", "-c"],
     "assembler" : "i686-elf-gcc"},
    {"name" : "Kernel",
     "src" : "kernel/src",
     "ldflagsAugment" : ["-T build/kernel.ld"],
     "linkerDependencies" : ["out/libmuzzle.a", "out/libfatfs.a", "out/libacpica.a"],
     "linkLogic" : "gcc"},
    {"name" : "NewlibInterface",
     "src" : "newlib/syscalls",
     "linkLogic" : "script",
     "includePaths" : ["include", "include/newlib"]},
    {"name" : "NewlibCrt0",
     "src" : "newlib/crt0",
     "includePaths" : ["include", "include/newlib"],
     "linkLogic" : "copy"},
    {"name" : "CxxSupport",
     "src" : "libcxxsup/src",
     "includePaths" : ["include", "include/newlib"],
     "linkerDependencies" : ["${NEWLIB}"]},
    {"name" : "EASTL",
     "src" : "third_party/EASTL/src",
     "cppflagsAgument" : ["-Wno-error=extra", "-Wno-error=deprecated-declarations"],
     "includePaths" : ["include", "include/newlib", "include/EASTL"],
     "linkerDependencies" : ["${NEWLIB}"]},
    {"name" : "Checkup",
     "src" : "checkup/src",
     "cppflagsAgument" : ["-Wno-error=extra", "-Wno-error=deprecated-declarations"],
     "includePaths" : ["${NEWLIB}"],
     "linkerDependencies" : ["${NEWLIB}"]},
    {"name" : "ShellSupport",
     "src" : "libshell/src",
     "cppflagsAgument" : ["-Wno-error=extra", "-Wno-error=deprecated-declarations"],
     "includePaths" : ["${NEWLIB}"],
     "linkerDependencies" : ["${NEWLIB}"]}
]
